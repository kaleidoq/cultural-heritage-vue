<view><view class="content"><view class="list"><block wx:for="{{addressList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',['$0'],[[['addressList','',index]]]]]]]}}" class="ml-20 row" bindtap="__e"><view class="left"><view class="head">{{''+row.head+''}}</view></view><view class="center"><view class="name-tel"><view class="name">{{row.name}}</view><view class="tel">{{row.tel}}</view><block wx:if="{{row.is_default}}"><view class="ml-10 cu-btn sm bg-green round">默认</view></block></view><view class="address">{{''+row.region.label+" "+row.address+''}}</view></view><view class="right"><view data-event-opts="{{[['tap',[['edit',['$0'],[[['addressList','',index]]]]]]]}}" class="icon cuIcon-edit" catchtap="__e"></view></view></view></block></view></view><view class="add"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="cu-btn lg round w-70 line-green" bindtap="__e"><view class="icon tianjia"></view>新增地址</view></view></view>